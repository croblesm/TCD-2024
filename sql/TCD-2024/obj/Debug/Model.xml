		<Element Type="SqlProcedure" Name="[dbo].[getAgenda]">
			<Property Name="BodyScript">
				<Value><![CDATA[
BEGIN
    SELECT a.*, s.*
    FROM Agenda a
    INNER JOIN Speakers s ON a.SpeakerId = s.SpeakerId
